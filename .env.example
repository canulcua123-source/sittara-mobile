# Sittara Mobile - Environment Variables Template
# Copy this file to .env and fill in the values

# API Configuration
EXPO_PUBLIC_API_URL=https://your-api-url.com

# Stripe Configuration (Publishable key - safe to expose in client)
# Get your publishable key from https://dashboard.stripe.com/apikeys
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# App Configuration
EXPO_PUBLIC_APP_NAME=Sittara
EXPO_PUBLIC_APP_VERSION=1.0.0
